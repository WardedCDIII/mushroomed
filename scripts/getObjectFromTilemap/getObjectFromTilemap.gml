// Script assets have changed for v2.3.0 see
// https://help.yoyogames.com/hc/en-us/articles/360005277377 for more information
function getObjectFromTilemap(idx){
	switch (idx) {
		case 0:
			return undefined;
		case 1:
			return obj_inkcap;
		case 2:
			return obj_toadstool;
		case 3:
			return obj_veiledlady;
		case 4:
			return obj_fly;
		case 5:
			return obj_frog;
		case 6:
			return obj_snail;
		case 7:
			return obj_pillbug;
		case 8:
			return obj_testenemy;
	}
}